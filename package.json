{
  "name": "sprouts",
  "version": "0.0.4",
  "description": "\"Sprouts\" paper-and-pencil game in your browser, by Kamil Mielnik",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "better-npm-run lint",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack-wizard",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint src/ --ext .js"
    },
    "start": {
      "command": "webpack-wizard-dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/sprouts.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.pl/"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/sprouts/issues"
  },
  "homepage": "https://github.com/kamilmielnik/sprouts#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.throttle": "^4.1.1",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sidebar": "git://github.com/kamilmielnik/sidebar.git"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "better-npm-run": "0.1.0",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "http-server": "^0.10.0",
    "react-hot-loader": "^3.1.1",
    "webpack-wizard": "git://github.com/kamilmielnik/webpack-wizard.git"
  }
}
