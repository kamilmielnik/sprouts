{
  "name": "sprouts",
  "version": "0.0.1",
  "description": "\"Sprouts\" paper-and-pencil game in your browser, by Kamil Mielnik",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "lint": "better-npm-run lint",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint src/ --ext .js"
    },
    "start": {
      "command": "node dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "http-server dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/sprouts.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.pl/"
  },
  "license": "CC BY-NC-ND 4.0",
  "bugs": {
    "url": "https://github.com/kamilmielnik/sprouts/issues"
  },
  "homepage": "https://github.com/kamilmielnik/sprouts#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash.throttle": "^4.1.1",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.1.0",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "style-ext-html-webpack-plugin": "^3.4.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
